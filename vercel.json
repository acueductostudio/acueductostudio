{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/static/assets/img/og/og_main.jpg",
      "destination": "/assets/img/og/og_main.jpg"
    }
  ],
  "redirects": [
    {
      "source": "/agendarconsultoria",
      "destination": "https://meetings.hubspot.com/hola250",
      "permanent": false
    },
    {
      "source": "/en/contacto",
      "destination": "/en/contact",
      "permanent": true
    },
    {
      "source": "/en/portafolio",
      "destination": "/en/work",
      "permanent": true
    },
    {
      "source": "/en/portafolio/:slug",
      "destination": "/en/work/:slug",
      "permanent": true
    },
    {
      "source": "/en/privacidad",
      "destination": "/en/privacy",
      "permanent": true
    },
    {
      "source": "/en/nosotros",
      "destination": "/en/about",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=43200, immutable"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}
