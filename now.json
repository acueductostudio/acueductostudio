{
  "name": "acueducto",
  "version": 2,
  "routes": [
    {
      "src": "/(.*)/",
      "status": 301,
      "headers": { "Location": "/$1" },
      "continue": true
    },
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },
    { "handle": "filesystem" }
  ]
}
